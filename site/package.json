		"imagemin-mozjpeg": "^9.0.0",
		"imagemin-pngquant": "^9.0.1",
		"imagemin-svgo": "^8.0.0",
		"inline-source": "^7.2.0",
		"postcss": "^7.0.32",
		"postcss-advanced-variables": "^3.0.1",
